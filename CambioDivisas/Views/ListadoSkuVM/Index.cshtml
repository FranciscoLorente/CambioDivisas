@model IEnumerable<CambioDivisas.Models.ViewModel.ListadoSkuVM>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Sku)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SumaTotal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Moneda)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Sku)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SumaTotal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Moneda)
        </td>
        <td>
            @Html.ActionLink("Ver transacciones", "VerTransacciones", new { Sku = item.Sku }) |
            @Html.ActionLink("Details", "Details", new { id = item.Sku })
        </td>
    </tr>
}

</table>
